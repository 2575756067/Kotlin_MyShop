<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.myshop">


    <uses-permission android:name="android.permission.INTERNET" />
    <application
        android:name="com.shop.app.MyApp"
        android:allowBackup="true"
        android:icon="@drawable/meinv_2"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".ui.home.NewBindActivity"></activity>
        <!--        动态栏详情-->
        <activity android:name="com.shop.ui.home.fragment.home.HomeChannelTypeActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <!--        品牌制造商 详情-->
        <activity android:name=".ui.brand.HomeBrandActivity" />
        <activity android:name=".tongpao.ManyLayoutActivity"></activity>
        <activity android:name="com.shop.ui.home.hom.BrandInfoActivity" />
        <activity android:name=".ui.newgoods.NewGoodsActivity" />
    </application>

</manifest>